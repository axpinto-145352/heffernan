    {
      "parameters": {
        "jsCode": "// Reformat to 5 rows per company + blank row\n// Run Once for All Items\n// For each company, outputs 5 rows:\n//   Row 1: Primary Name\n//   Row 2: Street Address\n//   Row 3: City, State, Zip\n//   Row 4: Domain\n//   Row 5: Bureau Number / POC Title\n//   Row 6: (blank separator)\n\nconst items = $input.all();\nconst out = [];\n\nfor (const item of items) {\n  const r = item.json || {};\n  const bureauNum = r['Bureau Number'] ?? '';\n  const primaryName = r['Primary Name'] ?? '';\n  const street = r['Street Address'] ?? '';\n  const city = r['City'] ?? '';\n  const state = r['State'] ?? '';\n  const zip = r['Zip Code'] ?? '';\n  const domain = r['Domain'] ?? r.company_domain ?? '';\n  const pocTitle = r['Title'] ?? r['POC Title'] ?? '';\n\n  // Row 1: Company Name\n  out.push({ json: { 'Bureau Number': bureauNum, 'Display': primaryName, 'Row Type': 'Company Name' } });\n  // Row 2: Street\n  out.push({ json: { 'Bureau Number': bureauNum, 'Display': street, 'Row Type': 'Street' } });\n  // Row 3: City State Zip\n  out.push({ json: { 'Bureau Number': bureauNum, 'Display': [city, state, zip].filter(v => v).join(', '), 'Row Type': 'City State Zip' } });\n  // Row 4: Domain\n  out.push({ json: { 'Bureau Number': bureauNum, 'Display': domain, 'Row Type': 'Domain' } });\n  // Row 5: Bureau # / POC Title\n  out.push({ json: { 'Bureau Number': bureauNum, 'Display': `Bureau: ${bureauNum} | POC Title: ${pocTitle}`, 'Row Type': 'Bureau & Title' } });\n  // Row 6: Blank separator\n  out.push({ json: { 'Bureau Number': '', 'Display': '', 'Row Type': 'Separator' } });\n}\n\nreturn out;\n"
